{
    "name": "sofwar/laravel-rabbitmq",
    "description": "Simple rabbitmq integration for Laravel",
    "license": "MIT",
    "authors": [
        {
            "name": "Adrian Tilita",
            "email": "adrian@tilita.ro",
            "role": "Developer"
        },
        {
            "name": "SofWar",
            "email": "ya.sofwar@yadnex.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "7.*|8.*",
        "php-amqplib/php-amqplib": "^3.*"
    },
    "require-dev": {
        "phpunit/phpunit": "9.*|10.*",
        "squizlabs/php_codesniffer": "3.7.*",
        "phpmd/phpmd": "@stable",
        "laravel/framework": "7.*|8.*|9.*|10.*",
        "phpstan/phpstan": "^1.10",
        "phpcompatibility/php-compatibility": "^9.3"
    },
    "autoload": {
        "psr-4": {
            "NeedleProject\\LaravelRabbitMq\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\NeedleProject\\LaravelRabbitMq\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "NeedleProject\\LaravelRabbitMq\\Providers\\ServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "allow-plugins": {
            "kylekatarnls/update-helper": true
        },
        "process-timeout": 1200
    }
}
